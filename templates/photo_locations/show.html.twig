{% extends 'base.html.twig' %}

{% block title %}PhotoLocations{% endblock %}

{% block body %}
    <h1 style="color: red">Photo Locations</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ photo_location.id }}</td>
            </tr>
            <tr>
                <th>Location</th>
                <td>{{ photo_location.location }}</td>
            </tr>
        </tbody>
    </table>

    <a class="btn btn-outline-primary btn-sm mt-2" href="{{ path('photo_locations_index') }}">Return to list</a>

    <a class="btn-outline-danger btn-sm mt-2" href="{{ path('photo_locations_edit', {'id': photo_location.id}) }}">Edit</a>

    {{ include('photo_locations/_delete_form.html.twig') }}
{% endblock %}
