{% extends 'base.html.twig' %}

{% block title %}Dog Skills{% endblock %}

{% block body %}
    <h1>Dog Skills</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ dog_skill.id }}</td>
            </tr>
            <tr>
                <th>SkillOrBehaviour</th>
                <td>{{ dog_skill.skillOrBehaviour }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ dog_skill.description }}</td>
            </tr>
            <tr>
                <th>Notes</th>
                <td>{{ dog_skill.notes }}</td>
            </tr>
        </tbody>
    </table>

    <a class="btn btn-outline-primary btn-sm" href="{{ path('dog_skills_index') }}">Return to list</a>

    <a class="btn btn-outline-primary btn-sm" href="{{ path('dog_skills_edit', {'id': dog_skill.id}) }}">Edit</a>

    {{ include('dog_skills/_delete_form.html.twig') }}
{% endblock %}
