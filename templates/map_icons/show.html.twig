{% extends 'base.html.twig' %}

{% block title %}Map Icon{% endblock %}

{% block body %}
    <h1 style="color: red">Map Icons</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ map_icon.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ map_icon.name }}</td>
            </tr>
            <tr>
                <th>IconFile</th>
                <td>{{ map_icon.iconFile }}</td>
            </tr>
        </tbody>
    </table>

    <a class="btn btn-outline-primary btn-sm" href="{{ path('map_icons_index') }}">Return to list</a>

    <a class="btn btn-outline-danger btn-sm" href="{{ path('map_icons_edit', {'id': map_icon.id}) }}">Edit</a>

    {{ include('map_icons/_delete_form.html.twig') }}
{% endblock %}
