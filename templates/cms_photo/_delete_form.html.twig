<form method="post" action="{{ path('cms_photo_delete', {'id': cms_photo.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ cms_photo.id) }}">
    <button class="btn btn-danger btn-sm">Delete</button>
</form>
