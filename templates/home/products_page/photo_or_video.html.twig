{% set loc = asset('administration/Attachments/CMSPhotos/')~cms_photo.photo %}
    {% if cms_photo.photoOrVideo == "Video" %}
        <video class="other-page-image" controls>
            <source src="{{ loc }}" type="video/mp4">
        </video>
    {% else %}

        <a target="_blank" href="{{ cms_photo.link }}">
            <img title="{{ cms_photo.title }}" class="homepage-image rotate-{{ cms_photo.rotate }}"
                 src="{{ loc }}"></a>

    {% endif %}


